{
  "name": "@nayms/contracts",
  "private": true,
  "version": "1.0.0",
  "description": "Nayms smart contracts",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "files": [
    "build/**",
    "contracts/**",
    "deployedAddresses.json"
  ],
  "scripts": {
    "devnet": "ganache-cli -a 10 -m 'funny door sample enrich female wedding stereo crane setup shop dwarf dismiss'",
    "devnet-ci": "ganache-cli -a 10 -m 'funny door sample enrich female wedding stereo crane setup shop dwarf dismiss' >ganache.log 2>&1 &",
    "devnet-0x": "docker run -it --rm -p 8545:8546 0xorg/devnet:constantinople-0.0.1",
    "truffle": "babel-node node_modules/.bin/truffle",
    "coverage": "yarn clean && MODE=coverage yarn test; istanbul report lcov",
    "profile": "yarn clean && MODE=profile yarn test; istanbul report lcov",
    "clean": "rm -rf coverage && yarn truffle networks --clean",
    "test": "yarn truffle test --network test",
    "trace": "MODE=trace yarn test",
    "compile": "yarn truffle compile",
    "migrate": "yarn truffle migrate",
    "sol-compiler": "sol-compiler",
    "sol-compiler:watch": "sol-compiler -w",
    "deploy:mainnet": "yarn truffle migrate --network mainnet && scripts/extractDeployedAddresses.js",
    "deploy:rinkeby": "yarn truffle migrate --network rinkeby && scripts/extractDeployedAddresses.js",
    "deploy:local": "yarn truffle migrate",
    "setup": "[[ -f .deployment.js ]] || cp .deployment-sample.js .deployment.js",
    "lint": "solhint --config ./.solhint.json contracts/**/*.sol",
    "abi-encode": "./scripts/abiEncode.js"
  },
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "ISC",
  "devDependencies": {
    "@0x/sol-compiler": "^3.1.9",
    "@0x/sol-coverage": "^3.0.6",
    "@0x/sol-profiler": "^3.1.8",
    "@0x/sol-trace": "^2.0.14",
    "argv": "0.0.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "commander": "^2.18.0",
    "coveralls": "^3.0.6",
    "delay": "^4.3.0",
    "eth-gas-reporter": "^0.1.9",
    "eth-hd-wallet": "^0.5.1",
    "ethereum-input-data-decoder": "0.0.3",
    "etherscan-api": "^3.0.11",
    "ethval": "^1.3.0",
    "faker": "^4.1.0",
    "get-stdin": "^7.0.0",
    "husky": "^3.0.3",
    "lodash": "^4.17.11",
    "moment": "^2.18.1",
    "prompt": "^1.0.0",
    "sol-merger": "^0.1.2",
    "solc": ">=0.5.8",
    "solgraph": "^0.3.2",
    "solhint": "^2.2.0",
    "solidity-coverage": "^0.6.4",
    "truffle": "^5.0.28",
    "truffle-hdwallet-provider": "^1.0.0-web3one.0",
    "uuid": "^3.0.1",
    "web3": "1.0.0-beta.33",
    "web3-eth-abi": "^1.0.0-beta.55",
    "web3-provider-engine": "^15.0.0",
    "web3-utils": "^1.0.0-beta.36",
    "yargs": "^9.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.2"
  },
  "engines": {
    "node": ">=10.15.1"
  }
}
